generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model BrowseSwiperProjects {
  id          BigInt    @id @default(autoincrement())
  image_url   String?
  projects_id BigInt?
  title       String?
  projects    projects? @relation(fields: [projects_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model SwiperProjects {
  id          BigInt    @id @default(autoincrement())
  image_url   String?
  location    String?
  projects_id BigInt?
  title       String?
  projects    projects? @relation(fields: [projects_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model projects {
  id                   BigInt   @id @default(autoincrement())
  project_img_url      String?
  title                String?
  sub_category_id      BigInt?
  BrowseSwiperProjects BrowseSwiperProjects[]
  SwiperProjects       SwiperProjects[]
  images               images[]
  projectDescription   projectDescription[]
  sub_category         sub_category? @relation(fields: [sub_category_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model projectDescription {
  id             BigInt    @id @default(autoincrement())
  title          String?
  location       String?
  plinth_area    String?
  project_status String?
  details        String?
  projects_id    BigInt?
  project        projects? @relation(fields: [projects_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model images {
  id          BigInt    @id @default(autoincrement())
  image_url   String?
  projects_id BigInt?
  project     projects? @relation(fields: [projects_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model contactForm {
  id      BigInt  @id @default(autoincrement())
  Name    String?
  Email   String?
  Number  BigInt?
  Message String?
}

model sub_category {
  id            BigInt     @id @default(autoincrement())
  category_name String?
  projects      projects[]
}
